def grader(a):
    if a>5 :
        if a>8:
            if a==10 :
                return ("A+")
            else:
                return ("A")
        elif   a>6:
            return ("A-")
        else:
             return ("B")
    if a> 3 and a<=5 :
        return ("c")
    elif a< 3:
        return ("f")




i=[1,2,5,6,9,2,1]
for m in i :
    print(grader(m))
